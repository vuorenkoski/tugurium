{
  "expo": {
    "name": "Tugurium",
    "slug": "Tugurium",
    "version": "0.3.0",
    "description": "Home automation client",
    "githubUrl": "https://github.com/vuorenkoski/tempview",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/newIcon3.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/newIcon3.png",
        "backgroundColor": "#FFFFFF"
      },
      "versionCode": 3,
      "package": "com.vuorenkoski.tuguriumMobile"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
